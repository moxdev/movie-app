{
  "extends": [
    // order here matters
    // "airbnb",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
